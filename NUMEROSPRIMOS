def primos_no_intervalo(a, b):
    primos = []
    for num in range(a, b + 1):
        if num > 1:  # números menores ou iguais a 1 não são primos
            eh_primo = True
            for i in range(2, num):
                if num % i == 0:
                    eh_primo = False
                    break
            if eh_primo:
                primos.append(num)
    return primos


##TESTE UNITARIO

import unittest

class TestPrimosNoIntervalo(unittest.TestCase):
    def test_intervalo_basico(self):
        self.assertEqual(primos_no_intervalo(1, 10), [2, 3, 5, 7])
    
    def test_sem_primos(self):
        self.assertEqual(primos_no_intervalo(14, 16), [])
    
    def test_intervalo_maior(self):
        self.assertEqual(primos_no_intervalo(10, 20), [11, 13, 17, 19])

if __name__ == '__main__':
    unittest.main()
